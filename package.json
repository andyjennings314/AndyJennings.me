{
  "name": "andyjennings-portfolio",
  "version": "1.0.0",
  "description": "Personal portfolio site for Andy Jennings",
  "main": "index.html",
  "scripts": {
    "start": "concurrently \"npm run watch:sass\" \"npm run watch:ts\" \"angular-http-server -o\"",
    "watch:sass": "nodemon -e scss -x \"sass css/andy.scss css/andy.css\"",
    "watch:ts": "nodemon -e ts -x \"tsc\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andyjennings314/AndyJennings.me.git"
  },
  "keywords": [
    "angularjs"
  ],
  "author": "Andy Jennings",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andyjennings314/AndyJennings.me/issues"
  },
  "homepage": "https://github.com/andyjennings314/AndyJennings.me#readme",
  "dependencies": {
    "@types/angular": "^1.6.56",
    "angular": "^1.8.0",
    "concurrently": "^5.0.0",
    "nodemon": "^2.0.0"
  },
  "devDependencies": {
    "typescript": "^3.7.2"
  }
}
